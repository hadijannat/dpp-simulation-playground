{
  "name": "dpp-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "contract:check": "npm run typegen && git diff --exit-code -- src/types/generated/platform-api.ts && node scripts/check-api-contracts.mjs",
    "test:e2e": "playwright test",
    "typegen": "openapi-typescript ../docs/api/openapi/platform-api.yaml -o src/types/generated/platform-api.ts",
    "test": "vitest --passWithNoTests"
  },
  "dependencies": {
    "@monaco-editor/react": "4.7.0",
    "@tanstack/react-query": "5.90.20",
    "i18next": "^25.8.4",
    "keycloak-js": "24.0.4",
    "monaco-editor": "0.48.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-i18next": "^16.5.4",
    "react-router-dom": "7.13.0",
    "reactflow": "11.10.1",
    "zustand": "5.0.11"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.0",
    "@eslint/js": "^10.0.1",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "@typescript-eslint/parser": "8.56.0",
    "@vitejs/plugin-react": "5.1.4",
    "eslint": "10.0.1",
    "globals": "17.3.0",
    "jsdom": "^28.0.0",
    "openapi-typescript": "^7.10.1",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "4.0.18",
    "web-vitals": "^5.1.0"
  }
}
