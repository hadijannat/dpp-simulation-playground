{
  "ci": {
    "collect": {
      "staticDistDir": "./frontend/dist",
      "url": ["http://localhost/"],
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.8 }],
        "categories:accessibility": ["error", { "minScore": 0.9 }]
      }
    }
  }
}
